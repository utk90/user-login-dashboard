    "@testing-library/user-event": "^12.8.3",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "axios": "^0.21.1",
    "bootstrap": "^4.6.0",
    "contentful": "^8.3.5",
    "enzyme": "^3.11.0",
    "formik": "^2.2.6",
    "jest-junit": "^12.0.0",
    "react": "^17.0.2",
    "react-app-rewired": "^2.1.8",
    "react-avatar": "^3.10.0",
    "react-bootstrap-validation": "^0.1.11",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.1.0",
    "react-lazyload": "^3.2.0",
    "react-password-toggle-icon": "1.1.4",
